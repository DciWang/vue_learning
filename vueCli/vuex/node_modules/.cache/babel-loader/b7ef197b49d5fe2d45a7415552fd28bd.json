CA,QAAA,OAAA,EAAA,SADA;AAEA,QAAA,OAFA,qBAEA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,MAAA;AACA;AAJA,OAAA;AAMA,KAzBA;;AA0BA;;AACA;;;;;;AAMA,IAAA,gBAjCA,8BAiCA;AACA,WAAA,KAAA;AACA,KAnCA;AAoCA,IAAA,gBApCA,8BAoCA;AACA,WAAA,KAAA;AACA,KAtCA;AAuCA,IAAA,MAvCA,oBAuCA;AACA,UAAA,CAAA,GAAA,IAAA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA;AACA,QAAA,IAAA,EAAA,QADA;AAEA,QAAA,CAAA,EAAA;AAFA,OAAA;AAIA,KA7CA;AA8CA,IAAA,OA9CA,qBA8CA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,UAAA,EAAA;AACA,QAAA,GAAA,EAAA;AADA,OAAA;AAGA;AAlDA,GATA;AA6DA,EAAA,UAAA,EAAA;AACA,IAAA,UAAA,EAAA;AADA;AA7DA,CAAA","sourcesContent":["<template>\n    <div id=\"app\">\n\n\n        <h1>-----App----</h1>\n        <h2>{{message}}</h2>\n        <h3>{{count}}</h3>\n        <h2>\n            应用vuex.store中的modules属性中的值\n        </h2>\n        <h3>{{$store.state.a.name}}</h3>\n        <h3>{{$store.getters.fullname}}</h3>\n        <h3>{{$store.getters.fullname2}}</h3>\n        <h3>{{$store.getters.fullname3}}</h3>\n        <button @click=\"aupname\">moudle中的action</button>\n        <button @click=\"upname()\">修改modules中的名字</button>\n        <h2>\n            应用vuex.store中的state属性中的值\n        </h2>\n        <h3>{{$store.state.counter}}</h3>\n        <h3>{{$store.state.persons}}</h3>\n        <h3>{{$store.state.info}}</h3>\n        <h2>\n            应用vuex.store中的getters属性中的值\n        </h2>\n        <h3>{{$store.getters.powerCount}}</h3>\n        <h3>{{$store.getters.more20Per}}</h3>\n        <button @click=\"pushPer()\">添加人员</button>\n        <button @click=\"updatePer()\">修改人员</button>\n        <h3>{{$store.getters.more20PerLength}}</h3>\n\n        <h3>{{$store.getters.morePerLengthCpn(10)}}</h3>\n\n\n        <!--不推荐-->\n        <button @click=\"increatmentClick()||$store.state.counter++\">不推荐+</button>\n        <button @click=\"decreatmentClick()||$store.state.counter--\">不推荐-</button>\n        <button @click=\"addcount(5)\">推荐+</button>\n        <button @click=\"delcount(5)\">推荐-</button>\n        <h1>-----HelloVue----</h1>\n\n        <!--以父子组件的方式传数据-->\n        <hellow-vuex :c-count=\"count\"/>\n    </div>\n</template>\n<script>\n    import HellowVuex from \"./views/HellowVuex\";\n\n    export default {\n        name: 'App',\n        data() {\n            return {\n                message: \"你好哇， vuex\",\n                count: 0\n            }\n        },\n        /*data 为负载 (payload)*/\n        methods: {\n            addcount(payload) {\n                this.$store.commit(\"ADDCOUNT\", payload)\n            },\n            delcount(payload) {\n                this.$store.commit(\"delCount\", payload)\n            },\n            pushPer() {\n                const newP = {id: 5, name: 'james', age:35}\n                const new1 = {id: 5, name: 'james', age:35}\n\n                this.$store.commit({\n                    type: \"addperson\",\n                    newP,\n                    new1,\n                })\n            },\n            //第一种方式\n            updatePer(){\n                this.$store.dispatch('updatePerson',{\n                    message:'我是携带的信息',\n                    success(){\n                        console.log('up成功')\n                    }\n                })\n            },\n            /*第二种方式*/\n         /*   updatePer(){\n               this.$store.dispatch('updatePerson','我是携带的信息').then(res =>{\n                   console.log('up成功');\n                   console.log(res);\n               })\n            },*/\n            increatmentClick() {\n                this.count++\n            },\n            decreatmentClick() {\n                this.count--\n            },\n            upname(){\n                const a=\"lv\"\n                this.$store.commit({\n                    type:'upname',\n                    a\n                })\n            },\n            aupname(){\n                this.$store.dispatch('aupnamew',{\n                    msg:'moudle 中的 action'\n                })\n            }\n        },\n        components: {\n            HellowVuex\n        }\n    }\n</script>\n<style>\n\n\n</style>\n"],"sourceRoot":"src"}]}